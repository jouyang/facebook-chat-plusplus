{
   "name": "Facebook-chat-plusplus",
   "version": "0.1.0",
   "manifest_version": 2,
   "content_scripts": [
      {
         "css": [
            "dist/main.css"
         ],
         "js": [
            "vendors/jquery/jquery.js",
            "dist/main.js"
         ],
         "matches": [
            "https://www.facebook.com/*",
            "http://www.facebook.com/*",
            "http://*.facebook.com/*",
            "https://*.facebook.com/*"
         ]
      }
   ],
   "icons": {
      "16": "icons/16x16.png",
      "48": "icons/48x48.png",
      "128": "icons/128x128.png"
   }
}
